{
  "name": "portalen-web",
  "description": "web frontend for portalen",
  "version": "2.0.0",
  "license": "MIT",
  "private": true,
  "author": "Telemark fylkeskommune (http://github.com/telemark)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/portalen-web"
  },
  "homepage": "https://github.com/telemark/portalen-web#readme",
  "keywords": [
    "Portalen",
    "Telemark fylkeskommune"
  ],
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "__DEVELOPMENT__",
      "webpackIsomorphicTools",
      "__DISABLE_SSR__",
      "__DEVTOOLS__",
      "__CLIENT__",
      "__SERVER__"
    ]
  },
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run start-prod\" \"npm run start-prod-api\"",
    "start-prod": "better-npm-run start-prod",
    "start-prod-api": "better-npm-run start-prod-api",
    "build": "better-npm-run build",
    "lint": "eslint -c .eslintrc src api",
    "start-dev": "better-npm-run start-dev",
    "start-dev-api": "better-npm-run start-dev-api",
    "watch-client": "better-npm-run watch-client",
    "dev": "NODE_ENV=development concurrently --kill-others \"npm run watch-client\" \"npm run start-dev\" \"npm run start-dev-api\"",
    "test": "nsp check && tap --reporter=spec test/**/*.js",
    "test-offline": "tap --reporter=spec test/**/*.js",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install",
    "test-node": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register",
    "test-node-watch": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register --watch"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080,
        "APIPORT": 3030
      }
    },
    "start-prod-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "APIPORT": 3030
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "start-dev-api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": 3030
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000,
        "APIPORT": 3030
      }
    },
    "build": {
      "command": "webpack --hide-modules --progress --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "babel-core": "6.26.3",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-react-display-name": "6.25.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "babel-runtime": "6.26.0",
    "better-npm-run": "0.1.0",
    "boom": "6.0.0",
    "classnames": "2.2.5",
    "concurrently": "3.6.1",
    "deep-equal": "1.0.1",
    "dotenv": "5.0.1",
    "flexboxgrid": "6.3.1",
    "good": "7.3.0",
    "good-console": "7.1.0",
    "good-squeeze": "5.0.2",
    "h2o2": "6.1.0",
    "hapi": "16.6.2",
    "hapi-auth-cookie": "7.0.0",
    "hapi-auth-jwt2": "7.4.1",
    "history": "4.7.2",
    "http-proxy": "1.17.0",
    "immutability-helper": "2.7.1",
